Cart = productId => {\n  let cart = [];\n  if (typeof window !== 'undefined') {\n    if (localStorage.getItem(\"cart\")) {\n      cart = JSON.parse(localStorage.getItem(\"cart\"));\n    }\n    cart.map((product, i) => {\n      if (product._id === productId) {\n        cart.splice(i, 1);\n      }\n    });\n    localStorage.setItem(\"cart\", JSON.stringify(cart));\n  }\n  return cart;\n};\n\nexport const cartEmpty = next => {\n  if (typeof window !== 'undefined') {\n    localStorage.removeItem(\"cart\");\n    let cart = [];\n    localStorage.setItem(\"cart\", JSON.stringify(cart));\n    next();\n  }\n};\n","/Users/srihitha/Desktop/mern/projfrontend/src/core/Cart.js",["164"],[],"/Users/srihitha/Desktop/mern/projfrontend/src/core/helper/OrderHelper.js",[],[],[],"/Users/srihitha/Desktop/mern/projfrontend/src/user/StripeCheckout.js",["165","166","167","168","169","170"],[],"/Users/srihitha/Desktop/mern/projfrontend/src/core/helper/paymentHelper.js",[],[],"/Users/srihitha/Desktop/mern/projfrontend/src/user/UserDashBoard.js",["171","172","173","174","175"],[],"/Users/srihitha/Desktop/mern/projfrontend/src/user/helper/userapicalls.js",[],[],{"ruleId":"176","severity":1,"message":"177","line":40,"column":6,"nodeType":"178","endLine":40,"endColumn":8,"suggestions":"179"},{"ruleId":"180","severity":1,"message":"181","line":28,"column":52,"nodeType":"182","messageId":"183","endLine":28,"endColumn":60},{"ruleId":"180","severity":1,"message":"184","line":28,"column":61,"nodeType":"182","messageId":"183","endLine":28,"endColumn":66},{"ruleId":"176","severity":1,"message":"185","line":47,"column":8,"nodeType":"178","endLine":47,"endColumn":10,"suggestions":"186"},{"ruleId":"176","severity":1,"message":"187","line":76,"column":7,"nodeType":"178","endLine":76,"endColumn":9,"suggestions":"188"},{"ruleId":"176","severity":1,"message":"189","line":38,"column":14,"nodeType":"178","endLine":38,"endColumn":16,"suggestions":"190"},{"ruleId":"191","severity":1,"message":"192","line":30,"colum